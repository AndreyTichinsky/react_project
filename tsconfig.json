{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "types/*": [
        "src/types/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "@emotion/core",
      "puppeteer",
      "jest-environment-puppeteer",
      "expect-puppeteer"
    ],
    "target": "es5",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true
  }
}
